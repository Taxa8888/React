{
    "name": "react_course",
    "version": "1.0.0",
    "description": "Study project, React, Redux, Webpack",
    "main": "index.js",
    "scripts": {
        "test": "jest",
        "test:watch": "jest --watch",
        "test:coverage": "jest --coverage",
        "build": "webpack --mode production",
        "dev": "webpack serve",
        "lint": "npx eslint ./src --ext .ts,.tsx",
        "check_types": "npx tsc --noEmit",
        "prepare": "npx husky install",
        "precommit": "npm run check_types && npm run lint"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Taxa8888/React.git"
    },
    "keywords": [
        "react",
        "redux",
        "js"
    ],
    "author": "Pavel Ponamarev",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/Taxa8888/React/issues"
    },
    "homepage": "https://github.com/Taxa8888/React#readme",
    "devDependencies": {
        "@babel/core": "7.14.3",
        "@babel/preset-env": "7.14.4",
        "@babel/preset-react": "7.13.13",
        "@babel/preset-typescript": "7.15.0",
        "@types/react": "17.0.18",
        "@types/react-dom": "17.0.9",
        "@typescript-eslint/eslint-plugin": "4.28.3",
        "@typescript-eslint/parser": "4.28.3",
        "babel": "6.5.2",
        "babel-core": "6.26.3",
        "babel-eslint": "10.1.0",
        "babel-jest": "27.4.2",
        "babel-loader": "8.2.2",
        "clean-webpack-plugin": "4.0.0-alpha.0",
        "css-loader": "5.2.6",
        "enzyme": "3.11.0",
        "enzyme-adapter-react-16": "^1.15.6",
        "enzyme-to-json": "3.6.2",
        "eslint": "7.30.0",
        "eslint-config-airbnb": "18.2.1",
        "eslint-config-prettier": "8.3.0",
        "eslint-import-resolver-typescript": "2.5.0",
        "eslint-plugin-import": "2.23.4",
        "eslint-plugin-jsx-a11y": "6.4.1",
        "eslint-plugin-prettier": "3.4.0",
        "eslint-plugin-react": "7.24.0",
        "eslint-plugin-react-hooks": "4.2.0",
        "eslint-plugin-unused-imports": "1.1.5",
        "file-loader": "6.2.0",
        "html-loader": "2.1.2",
        "html-webpack-plugin": "5.3.1",
        "husky": "7.0.2",
        "jest": "27.0.3",
        "mini-css-extract-plugin": "1.6.0",
        "prettier": "2.3.2",
        "redux-devtools-extension": "2.13.9",
        "sass": "1.35.0",
        "sass-loader": "12.1.0",
        "webpack": "5.38.1",
        "webpack-cli": "4.7.0",
        "webpack-dev-server": "4.6.0"
    },
    "dependencies": {
        "@types/react-router": "5.1.16",
        "@types/react-router-dom": "5.3.0",
        "react": "17.0.2",
        "react-dom": "17.0.2",
        "react-loading": "2.0.3",
        "react-redux": "7.2.4",
        "react-router": "5.2.0",
        "react-router-dom": "5.3.0",
        "redux": "4.1.0",
        "redux-thunk": "2.3.0",
        "typescript": "4.3.5"
    },
    "jest": {
        "verbose": true,
        "clearMocks": true,
        "collectCoverage": true,
        "setupFilesAfterEnv": [
            "./src/testconfig/test.config.js"
        ],
        "snapshotSerializers": [
            "./node_modules/enzyme-to-json/serializer"
        ],
        "transform": {
            "^.+\\.js$": "babel-jest"
        },
        "moduleNameMapper": {
            "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "./__mocks__/fileMock.js",
            "\\.(css|scss)$": "identity-obj-proxy"
        }
    }
}
